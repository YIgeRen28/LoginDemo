body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, textarea, p, blockquote, th, td {
    margin: 0;
    padding: 0;
}
li{
	list-style: none;
}
a{
	text-decoration:none;
	color: #333;
}
i{
	font-style: normal;
}
em{
	font-style: normal;
}
img{
	border:0;
}
.header{
	width: 100%;
	height: 48px;
	background: gray;
	/*position: fixed;*/
	overflow: hidden;
	top: 0;
	z-index: 100;
}
