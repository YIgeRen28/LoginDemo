					.you_friend{
						padding: 16px;
						background:#f2f2f5;
						overflow: hidden;
						.pic{
							float: left;
							width: 30px;
							height: 30px;
							overflow: hidden;
							img{
								width: 100%;
								height: 100%;
							}
						}
						.con{
							margin-left: 40px;
							min-height: 30px;
							line-height: 30px;
							position: relative;
							.fri_name{
								height: 14px;
								line-height: 14px;
								/*line-height: 1.2;*/
								font-size: 12px;
								padding: 16px 10px 8px 16px;
								a{
									color: #eb7350;
									line-height: 14px;
									i{
										background: url(../images/bg_listNum1.png) no-repeat left center;
										background-size: 475px 14px;
										width: 15px;
										height: 14px;
										margin-left: 3px;
										vertical-align: -2px;
									}
								}
							}
							.fri_classify{

								width: 8em;
								height: 1.2em;
								line-height: 1.2em;
								margin-top: 3px;
								color: #808080;
								overflow: hidden;
								text-overflow: ellipsis;
								white-space: nowrap;
								word-wrap: normal;
							}
							.fri_attention{
								position: absolute;
								right: 0;
								top: 0;
								a{
									display: inline-block;
									text-align: center;
									font-size: 12px;
									padding: 0 5px 0 5px;
									border:1px solid #d9d9d9;
									background: #fff;
									box-shadow: 0px 1px 2px rgba(0,0,0,0.1);
									border-radius: 2px;
									height: 24px;
									line-height: 24px;
									i{
										color: #fa7d3c;
										line-height: 24px;
										font-size: 20px;
										overflow: hidden;
										margin-right: 4px;
										vertical-align: middle;
									}
								}
							}
						} 
					}
				}
			}
		}

	}

}